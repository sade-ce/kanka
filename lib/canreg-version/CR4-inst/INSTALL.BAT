@ECHO OFF
ECHO     INSTALLING CANREG-4
ECHO    (Hit Ctrl-C to cancel)
PAUSE
rem ------------------------------------------ MAKE DIRECTORIES
MD \CR4SHARE
MD \CR4SHARE\CANREG4
MD \CR4SHARE\CANREG4\CR4-EXEC
MD \CR4SHARE\CANREG4\CR4-LANG
MD \CR4SHARE\CANREG4\CR4-LANG\E
MD \CR4SHARE\CANREG4\CR4-LANG\F
MD \CR4SHARE\CANREG4\CR4-LANG\S
MD \CR4SHARE\CANREG4\CR4-LANG\I
MD \CR4SHARE\CANREG4\CR4-LANG\T
MD \CR4SHARE\CANREG4\CR4-LANG\C
MD \CR4SHARE\CANREG4\CR4-LANG\H
MD \CR4SHARE\CANREG4\CR4-LANG\A
MD \CR4SHARE\CANREG4\CR4-LANG\Z
MD \CR4SHARE\CANREG4\CR4-SYST
MD \CR4SHARE\CANREG4\CR4-BACK
MD \CR4SHARE\CANREG4\CR4-WORK
rem --------------------------------------- COPY ZIP FILES
DEL \CR4SHARE\CANREG4\CR4-EXEC\*.PMD
MOVE *.PMD \CR4SHARE\CANREG4\CR4-EXEC
DEL \CR4SHARE\CANREG4\CR4-EXEC\*.BGI
MOVE *.BGI \CR4SHARE\CANREG4\CR4-EXEC
DEL \CR4SHARE\CANREG4\CR4-EXEC\*.DEF
MOVE *.DEF \CR4SHARE\CANREG4\CR4-EXEC
DEL \CR4SHARE\CANREG4\CR4-EXEC\P*.EXE
MOVE P*.EXE \CR4SHARE\CANREG4\CR4-EXEC
DEL \CR4SHARE\CANREG4\CR4-EXEC\*.ZIP
MOVE *.ZIP \CR4SHARE\CANREG4\CR4-EXEC
rem -------------------------------------- DELETE PREVIOUS FILES
CD \CR4SHARE\CANREG4\CR4-EXEC
DEL CR4*.EXE
DEL ANALYSIS.*
DEL *.DLL
DEL *.BGI
DEL *.BMP
DEL *.BPL
DEL *.JF 
REM --------------------------------------- UNZIP DLLs, PACKAGES
PPMD d BANTAM.PMD
PPMD d IDAPI32.PMD
PPMD d IDPDX32.PMD
PPMD d IDR20009.PMD
PPMD d VCL60.PMD
PPMD d VCLDB60.PMD
PPMD d RTL60.PMD
PPMD d DBRTL60.PMD
PPMD d BDERTL60.PMD
PPMD d BORLNDMM.PMD
PPMD d CC3260MT.PMD
rem -------------------------------------------- UNZIP EPI INFO 
PPMD d ANAL-EXE.PMD
PPMD d ANAL-HLP.PMD
rem --------------------------------- UNZIP CANREG4 EXECUTABLES
PPMD d CR4_INIT.PMD
PPMD d CR4_MENU.PMD
PPMD d CR4_EDIT.PMD
PPMD d CR4_DICT.PMD
PPMD d CR4_IMPT.PMD
PPMD d CR4_EXPT.PMD
PPMD d CR4_FREQ.PMD
PPMD d CR4_TABL.PMD
PPMD d CR4_STAT.PMD
PPMD d CR4_VWKF.PMD
PPMD d CR4_BACK.PMD
PPMD d CR4_FNAS.PMD
PPMD d CR4_PERS.PMD
PPMD d CR4_USER.PMD
rem -------------------------------------------- UNZIP IMAGES
PPMD d CR4-WHO.PMD
PPMD d CR4-IARC.PMD
PPMD d CR4-IACR.PMD
PPMD d CR4-ACCP.PMD
rem ------------------------------ DELETE ALL PMD ZIPPED FILES
DEL *.PMD
rem --------------------------------- UNZIP JF (CHECK/CONVERT)
PKZIP25 -extract JF
DEL JF.ZIP
rem --------------------------------- COPY PKZIPPED LANG FILES
rem ---- ENGLISH
DEL \CR4SHARE\CANREG4\CR4-LANG\E\E.ZIP
MOVE E.ZIP \CR4SHARE\CANREG4\CR4-LANG\E
rem ---- FRENCH
DEL \CR4SHARE\CANREG4\CR4-LANG\F\F.ZIP
MOVE F.ZIP \CR4SHARE\CANREG4\CR4-LANG\F
rem ---- SPANISH
DEL \CR4SHARE\CANREG4\CR4-LANG\S\S.ZIP
MOVE S.ZIP \CR4SHARE\CANREG4\CR4-LANG\S
rem ---- ITALIAN
DEL \CR4SHARE\CANREG4\CR4-LANG\I\I.ZIP
MOVE I.ZIP \CR4SHARE\CANREG4\CR4-LANG\I
rem ---- TURKISH
DEL \CR4SHARE\CANREG4\CR4-LANG\T\T.ZIP
MOVE T.ZIP \CR4SHARE\CANREG4\CR4-LANG\T
rem ---- THAI
DEL \CR4SHARE\CANREG4\CR4-LANG\H\H.ZIP
MOVE H.ZIP \CR4SHARE\CANREG4\CR4-LANG\H
rem ---- CHINESE
DEL \CR4SHARE\CANREG4\CR4-LANG\C\C.ZIP
MOVE C.ZIP \CR4SHARE\CANREG4\CR4-LANG\C
rem ---- ARABIC
DEL \CR4SHARE\CANREG4\CR4-LANG\A\A.ZIP
MOVE A.ZIP \CR4SHARE\CANREG4\CR4-LANG\A
rem ---- FARSI / PERSIAN
DEL \CR4SHARE\CANREG4\CR4-LANG\Z\Z.ZIP
MOVE Z.ZIP \CR4SHARE\CANREG4\CR4-LANG\Z
rem ---- LANGUAGE LIST
DEL \CR4SHARE\CANREG4\CR4-LANG\CR4-LANG.DEF
MOVE CR4-LANG.DEF \CR4SHARE\CANREG4\CR4-LANG
rem ------------------------------------------ UNZIP LANG FILES
CD ..\CR4-LANG
CD E
DEL *.LTS
DEL *.AID
\CR4SHARE\CANREG4\CR4-EXEC\PKZIP25 -extract E
DEL E.ZIP
CD ..\F
DEL *.LTS
DEL *.AID
