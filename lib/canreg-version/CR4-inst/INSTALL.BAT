DEL *.LTS
DEL *.AID
\CR4SHARE\CANREG4\CR4-EXEC\PKZIP25 -extract H
DEL H.ZIP
CD ..\C
DEL *.LTS
DEL *.AID
\CR4SHARE\CANREG4\CR4-EXEC\PKZIP25 -extract C
DEL C.ZIP
CD ..\A
DEL *.LTS
DEL *.AID
\CR4SHARE\CANREG4\CR4-EXEC\PKZIP25 -extract A
DEL A.ZIP
CD ..\Z
